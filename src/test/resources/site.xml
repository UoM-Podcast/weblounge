<?xml version="1.0" encoding="UTF-8"?>
<site id="dev" xmlns="http://www.entwinemedia.com/weblounge/3.0/site" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.entwinemedia.com/weblounge/3.0/site http://www.entwinemedia.com/xsd/weblounge/3.0/site.xsd">
  <autostart>true</autostart>
  <name><![CDATA[Main site]]></name>
  <languages>
    <language default="true">de</language>
    <language>en</language>
  </languages>
  <domains>
  	<url default="true">http://www.weblounge.org</url>
  	<url>http://*.nowhere.com</url>
    <url>http://localhost:8080</url>
  </domains>
  <security>
    <administrator>
      <login>blogadmin</login>
      <password type="plain"><![CDATA[secret]]></password>
      <name><![CDATA[Web Office]]></name>
      <email>weboffice@nowhere.com</email>
    </administrator>
    <authentication>
    <loginmodule>
      <class>ch.entwine.weblounge.common.impl.security.jaas.AdminLoginModule</class>
      <relevance>sufficient</relevance>
    </loginmodule>
    </authentication>
  </security>
  <templates>
    <template id="default" composeable="true" default="true">
      <name><![CDATA[Default template]]></name>
      <renderer>file://template/default.jsp</renderer>
      <stage>boxes</stage>
      <recheck>1d10M</recheck>
      <valid>1w1d1H1M</valid>
    </template>
    <template id="mobile" composeable="true">
      <name><![CDATA[Mobile]]></name>
      <renderer>file://template/mobile.jsp</renderer>
      <recheck>1H</recheck>
      <valid>1w</valid>
    </template>
  </templates>
  <options>
    <option>
      <name>media.url</name>
      <value><![CDATA[http://media.swissunihockey.ch/dev]]></value>
    </option>
  </options>
</site>