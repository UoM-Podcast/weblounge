<div id="module_tabs">
	<ul>
		<li><a id="favorites" href="#module_favorites">Favoriten</a></li>
		{{each modules}}
			<li><a id="${$value.id}" href="#tabcontent">${$value.name}</a></li>
    	{{/each}}
	</ul>
	<a class="tab_close" href="#" role="button"></a>
	<div id="module_favorites">
	{{each favorites}}
		<div id="${$value.name}" class="draggable ui-widget-content" module="${$value.module}" style="display: block;">${$value.name}</div>
	{{/each}}
	</div>
	<div id="tabcontent"></div>
</div>