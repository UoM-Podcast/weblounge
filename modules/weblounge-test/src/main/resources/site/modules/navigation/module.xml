<?xml version="1.0" encoding="UTF-8"?>
<module id="navigation" xmlns="http://www.entwinemedia.com/weblounge/3.2/module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.entwinemedia.com/weblounge/3.2/module http://www.entwinemedia.com/xsd/weblounge/3.2/module.xsd">

  <enable>true</enable>
  <name>Navigation</name>

  <pagelets>

    <pagelet id="link" composeable="true">
      <name>Link</name>
      <renderer>file://${module.root}/jsp/link/link.jsp</renderer>
      <editor>file://${module.root}/jsp/link/link_editor.jsp</editor>
      <includes>
        <link use="editor" href="file://${module.root}/jsp/link/link.css" rel="stylesheet" type="text/css"></link>
        <script use="editor" src="file://${module.root}/jsp/link/link.js" type="text/javascript"></script>
      </includes>
    </pagelet>

    <pagelet id="linklist" composeable="true">
      <name>Link list</name>
      <renderer>file://${module.root}/jsp/linklist/list.jsp</renderer>
      <editor>file://${module.root}/jsp/linklist/list_editor.jsp</editor>
      <recheck>1H</recheck>
      <valid>2H</valid>
    </pagelet>

  </pagelets>

</module>